FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

SOLVER
{
    localCFL        10.0;
    relaxOmega      1.2;
    tolerance       1e-12;
    relTol          0.001;
    searchDir       8;
    innerIter       10;
}

