FoamFile
{
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform (1 0 0);

boundaryField
{
    upper
    {
        type            cyclic;
    }

    down
    {
        type            cyclic;
    }

    left
    {
        type            cyclic;
    }

    right
    {
        type            cyclic;
    }

    frontAndBack
    {
        type            empty;
    }
}

